          section#demo
            .page-header
              h1 Getting Started
            p
              a(href="https://github.com/FGRibreau/node-childrens/tree/master/examples") See the working example code  
              | . We accept pull-requests!

          section#who
            .page-header
              h1 Who are you?
              p My name is Francois-Guillaume Ribreau, I 
                a(href="http://twitter.com/FGRibreau") discover, share (twitter)
                | , 
                a(href="http://blog.fgribreau.com/") learn (blog)
                | , 
                a(href="https://github.com/FGRibreau?tab=repositories") code (github)
                | , 
                a.bringr(href="http://bringr.net", data-placement='top', title="Outil d'analyse des réseaux sociaux") d
                a.redsmin(href="http://redsmin.com", data-placement='top', title="Fully featured GUI for Redis") o
                |.
          - if (dox.length > 0)
            section#who
              .page-header
                h1 Documentation

          each symbol in dox
            section(id=symbol.ctx.name)
                h1= symbol.ctx.name
                if !symbol.gtype && symbol.type
                  p
                    div(class=["label", "label-inverse", "radius", "ctx-type"])= symbol.type
                else
                  p
                    mixin labelForSymbolType(symbol)
                    if symbol.ctx.string
                      span= symbol.ctx.string
                    if symbol.return
                      |  -> 
                      span= symbol.return

              if symbol.hasParams
                table.table.table-bordered.table-striped
                  thead
                    tr
                      th(style="width:20%") Option name
                      th(style="width:20%") Type
                      th Description
                  tbody
                    each tag in symbol.tags
                      if tag.type == 'param'
                        tr
                          td= tag.name
                          td= tag.types
                          td= tag.description

              .description !{symbol.description.full} !{symbol.description.extra}

              pre
                code.language-javascript
                  = symbol.code
         
