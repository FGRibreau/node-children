    div.navbar.navbar-inverse.navbar-fixed-top
      div.navbar-inner
        div.container
          a.brand Node Childrens
          div.nav-collapse.collapse
            ul.nav.pull-right.sponsored
              li.inter
                p Development sponsored by
              li.sponsor.sponsor-bringr
                a.bringr(href='http://bringr.net/?btt', data-placement="bottom", title="Outil d'analyse des réseaux sociaux") Bringr
              li.inter
                p and
              li.sponsor.sponsor-redsmin
                a.redsmin(href='https://redsmin.com/?btt', data-placement="bottom", title="Fully featured GUI for Redis") Redsmin


    header.jumbotron.subhead#overview
      div.container
        h1 Node Childrens
        p.lead Concurrent tasks computation among nodejs child processes (vertical scaling)

    .container
      .row
        .span3.bs-docs-sidebar
          ul.nav.nav-list.bs-docs-sidenav.affix-top
            each file in files
              li(class=(file.name !== currentName ? "":"active"))
                a(href=file.targetName) #{file.name}

          .scrollspy
            ul.nav.nav-list.bs-docs-sidenav.affix-top
              li
                a(href='#what') What/Getting Started/Who ?
              each symbol in symbols
                li
                  if name === currentName
                    a(href='#'+symbol.name)
                      mixin iForSymbolType(symbol) #{symbol.name}
                  else
                    a(href=symbol.targetFile+'#'+symbol.name)
                      mixin iForSymbolType(symbol) #{symbol.name}

        .span9
          section#opt
            iframe(src="http://ghbtns.com/github-btn.html?user=fgribreau&repo=node-childrens&type=watch&count=true&size=large",  allowtransparency="true", frameborder="0", scrolling="0", width="140", height="30")
            iframe(src="http://ghbtns.com/github-btn.html?user=fgribreau&repo=node-childrens&type=fork&count=true&size=large",   allowtransparency="true", frameborder="0", scrolling="0", width="140", height="30")
            iframe(src="http://ghbtns.com/github-btn.html?user=fgribreau&repo=node-childrens&type=follow&count=true&size=large", allowtransparency="true", frameborder="0", scrolling="0", width="300", height="30")
            a(href="https://twitter.com/intent/tweet?button_hashtag=node-childrens&text=Concurrent%20tasks%20computation%20among%20nodejs%20child%20processes%20%2Fby%20%40fgribreau", class="twitter-hashtag-button", data-size="large", data-related="FGRibreau", data-url="http://fgribreau.github.com/node-childrens/docs/node-childrens.js.html") Share on Twitter
            
          //- section#what
          //-   .page-header
          //-     h1 What provides node-childrens ?
          //-   ul
          //-     p Clear Separation of Concerns
